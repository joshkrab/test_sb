<!DOCTYPE html>
<html lang="ru">
   <head>
      <title>Kushop</title>
      <meta charset="UTF-8" />
      <meta name="format-detection" content="telephone=no" />
      <link rel="stylesheet" href="css/style.css" />
      <link rel="shortcut icon" href="favicon.ico" />
      <!-- <meta name="robots" content="noindex, nofollow"> -->
      <!-- <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"> -->
      <meta name="viewport" content="width=device-width, initial-scale=1.0" />
   </head>
   <body>
      <div class="wrapper">
         <!-- @import "header.kit" -->
         <!-- First section -->
         <section class="first-section">
            <div class="container">
               <h2>Section 1</h2>
            </div>
         </section>
         <section class="second-section">
            <div class="container">
               <h2>Section 2</h2>
            </div>
         </section>
         <div class="popup1">
            <div class="popup1__header">
               <h2 class="popup1__title">Products</h2>
               <div class="popup1__close">
                  <div class="popup1__close-item"></div>
               </div>
            </div>
            <div class="popup1__cards pop-card">
               <div class="pop-card__item"></div>
               <div class="pop-card__item"></div>
               <div class="pop-card__item"></div>
               <div class="pop-card__item"></div>
            </div>
            <div class="popup1__sub-total">
               <p>Sub-total</p>
               <p>$ 7.95</p>
            </div>
            <div class="line"></div>
            <div class="popup1__total">
               <p>Total</p>
               <p>$ 7.95</p>
            </div>

            <div class="popup1__buttons">
               <a href="#" class="quick">Quick order</a>
               <a href="#" class="regular">Regular order</a>
            </div>
         </div>
         <!-- @import "footer.kit" -->
      </div>
      <!-- @import "popup.kit" -->
      <!-- @import "js.kit" -->
   </body>
</html>
